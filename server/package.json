{
  "name": "node-api",
  "version": "1.0.0",
  "description": "simple node api",
  "license": "MIT",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "dev": "docker rm -f corner || true && docker rmi -f kevinkoste/corner || true && tsc && docker build -t kevinkoste/corner . && docker run --publish 8000:8000 --name corner kevinkoste/corner:latest",
    "push": "tsc && docker build -t kevinkoste/corner . && docker push kevinkoste/corner",
    "start": "node -r dotenv/config --es-module-specifier-resolution=node dist/app.js",
    "build": "tsc",
    "deploy": "heroku container:push web -a corner-server && heroku container:release web -a corner-server",
    "create-tables": "aws cloudformation create-stack --stack-name corner-tables --template-body file://src/resources/tables.yml",
    "create-stack": "aws cloudformation create-stack --stack-name corner --template-body file://src/resources/resources.yml --parameters ParameterKey=CornerKeyPair,ParameterValue=CornerKeyPair",
    "update-stack": "aws cloudformation update-stack --stack-name corner --template-body file://src/resources/resources.yml --parameters ParameterKey=CornerKeyPair,ParameterValue=CornerKeyPair"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.2.4",
    "aws-sdk": "^2.738.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cotter-node": "^0.1.2",
    "cotter-token-js": "^0.1.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "file-type": "^15.0.0",
    "fs": "^0.0.1-security",
    "multiparty": "^4.2.2",
    "pg": "^8.3.2",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/multiparty": "^0.0.32",
    "@types/node": "^14.6.2",
    "@types/pg": "^7.14.4",
    "@types/uuid": "^8.3.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  }
}
