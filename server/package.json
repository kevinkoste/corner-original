{
  "name": "node-api",
  "version": "1.0.0",
  "description": "simple node api",
  "license": "MIT",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "dev": "docker rm -f corner || true && docker rmi -f kevinkoste/corner || true && tsc && docker build -t kevinkoste/corner . && docker run --publish 8000:8000 --name corner kevinkoste/corner:latest",
    "push": "tsc && docker build -t kevinkoste/corner . && docker push kevinkoste/corner",
    "start": "node -r dotenv/config --es-module-specifier-resolution=node dist/app.js",
    "build": "tsc",
    "deploy": "heroku container:push web -a corner-server && heroku container:release web -a corner-server",
    "deploy-staging": "heroku container:push web -a corner-server-staging && heroku container:release web -a corner-server-staging",
    "create-tables": "aws cloudformation update-stack --stack-name corner-tables --template-body file://src/resources/tables.yml"
  },
  "dependencies": {
    "@magic-sdk/admin": "^1.2.2",
    "@sendgrid/mail": "^7.2.4",
    "aws-sdk": "^2.738.0",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fs": "^0.0.1-security",
    "multiparty": "^4.2.2",
    "passport": "^0.4.1",
    "passport-magic": "^1.0.0",
    "sharp": "^0.26.2",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.21",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/express-session": "^1.17.0",
    "@types/multiparty": "^0.0.32",
    "@types/node": "^14.6.2",
    "@types/passport": "^1.0.4",
    "@types/passport-strategy": "^0.2.35",
    "@types/sharp": "^0.26.0",
    "@types/uuid": "^8.3.0",
    "nodemon": "^2.0.4",
    "request": "^2.88.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  }
}
