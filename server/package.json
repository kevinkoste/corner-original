{
  "name": "node-api",
  "version": "1.0.0",
  "description": "simple node api",
  "license": "MIT",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "start": "tsc && node -r dotenv/config --es-module-specifier-resolution=node dist/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "reset-resources": "yarn deploy && yarn populate",
    "deploy": "aws cloudformation update-stack --stack-name linker --template-body file://src/resources/dynamodb-resources.yml",
    "populate": "aws dynamodb put-item --table-name profiles --item file://src/resources/kauber.json"
  },
  "dependencies": {
    "@types/uuid": "^8.3.0",
    "aws-sdk": "^2.738.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cotter-node": "^0.1.2",
    "cotter-token-js": "^0.1.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "file-type": "^15.0.0",
    "fs": "^0.0.1-security",
    "multiparty": "^4.2.2",
    "pg": "^8.3.2",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/multiparty": "^0.0.32",
    "@types/node": "^14.6.0",
    "@types/pg": "^7.14.4",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  }
}
